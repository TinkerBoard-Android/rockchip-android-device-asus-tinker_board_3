/dts-v1/;
/plugin/;
/ {
	compatible = "rockchip,rk3568";

	fragment@0 {
		target = <&pwm6>;
		__overlay__ {
			status = "okay";
		};
	};
	fragment@1 {
		target = <&backlight>;
		__overlay__ {
			status = "okay";
			panel-type = "LVDS";
			pwms = <&pwm6 0 1000000 0>;
			minimal-brightness-level = <26>;
			brightness-levels = <
					255 254 253 252 251 250 249 248
					247 246 245 244 243 242 241 240
					239 238 237 236 235 234 233 232
					231 230 229 228 227 226 225 224
					223 222 221 220 219 218 217 216
					215 214 213 212 211 210 209 208
					207 206 205 204 203 202 201 200
					199 198 197 196 195 194 193 192
					191 190 189 188 187 186 185 184
					183 182 181 180 179 178 177 176
					175 174 173 172 171 170 169 168
					167 166 165 164 163 162 161 160
					159 158 157 156 155 154 153 152
					151 150 149 148 147 146 145 144
					143 142 141 140 139 138 137 136
					135 134 133 132 131 130 129 128
					127 126 125 124 123 122 121 120
					119 118 117 116 115 114 113 112
					111 110 109 108 107 106 105 104
					103 102 101 100  99  98  97  96
					95  94  93  92  91  90  89  88
					87  86  85  84  83  82  81  80
					79  78  77  76  75  74  73  72
					71  70  69  68  67  66  65  64
					63  62  61  60  59  58  57  56
					55  54  53  52  51  50  49  48
					47  46  45  44  43  42  41  40
					39  38  37  36  35  34  33  32
					31  30  29  28  27  26  25  24
					23  22  21  20  19  18  17  16
					15  14  13  12  11  10   9   8
					7   6   5   4   3   2    2   0>;
		};
	};
	fragment@2 {
		target = <&i2c4>;
		__overlay__ {
			status = "okay";
			dsi_lvds_bridge2: lt9211@2d {
				status = "okay";
				dsi,lanes = <4>;
				lvds-format = <2>;//VESA
				lvds-bpp = <18>;
				Tinker3;
				enable-overlay;
				//test-pattern;
			};
		};
	};
	fragment@3 {
		target = <&dsi0>;
		__overlay__ {
			status = "okay";
			dsi0_panel: panel@0 {
				backlight = <&backlight>;

				width-mm = <152>;
				height-mm = <91>;
				bpc=<6>;
				bus-format = <0x1009>;//<MEDIA_BUS_FMT_RGB666_1X18>;
				dsi,flags = <0x03>;//101000000011
				dsi,format = <0>;//MIPI_DSI_FMT_RGB888:
				dsi,lanes = <4>;

				t1=<1000>;//VCC on to start lvds signal
				t2=<2100>;//t2-t6: LVDS signal(start) to turn Backlihgt on or Backlight sys Enable
				t3=<1000>;//Backlight sys Disable or Backlihgt(off) to stop lvds signal
				t4=<1000>;//LVDS signal to turn VCC off
				t5=<1000>;//VCC off to turn VCC on
				t6=<0>;//Backlight sys Enable to turn Backlight on
				t7=<0>;//Backlight off to Backlight sys Disable
				//t1 -> t2-t6 -> t6 -> t3 -> t7 -> t4 -> t5

				disp_timings0: display-timings {
					native-mode = <&dsi0_timing0>;

					dsi0_timing0: timing0  {
						clock-frequency = <33300000>;
						hactive = <800>;
						vactive = <480>;
						hfront-porch = <102>;
						hback-porch = <102>;
						hsync-len = <52>;
						vfront-porch = <18>;
						vsync-len = <9>;
						vback-porch = <18>;
						hsync-active = <0>;
						vsync-active = <0>;
						de-active = <1>;
						pixelclk-active = <0>;
					};
				};
			};
		};
	};
	fragment@4 {
		target = <&edp>;
		__overlay__ {
			status = "disabled";
		};
	};
	fragment@5 {
		target = <&edp_phy>;
		__overlay__ {
			status = "disabled";
		};
	};
	fragment@6 {
		target = <&panel_edp>;
		__overlay__ {
			status = "disabled";
		};
	};
	fragment@7 {
		target = <&dsi0>;
		__overlay__ {
			status = "okay";
		};
	};
	fragment@8 {
		target = <&dsi0_in_vp1>;
		__overlay__ {
			status = "okay";
		};
	};
	fragment@9 {
		target = <&edp_in_vp1>;
		__overlay__ {
			status = "disabled";
		};
	};
};
