/dts-v1/;
/plugin/;
/ {
	compatible = "rockchip,rk3568";

	fragment@0 {
		target = <&pwm6>;
		__overlay__ {
			status = "okay";
		};
	};
	fragment@1 {
		target = <&backlight>;
		__overlay__ {
			status = "okay";
			panel-type = "LVDS";
			pwms = <&pwm6 0 1000000 0>;
			minimal-brightness-level = <26>;
		};
	};
	fragment@2 {
		target = <&i2c4>;
		__overlay__ {
			status = "okay";
			dsi_lvds_bridge2: lt9211@2d {
				status = "okay";
				dsi,lanes = <4>;
				lvds-format = <2>;//VESA
				lvds-bpp = <18>;
				Tinker3;
				enable-overlay;
				//test-pattern;
			};
		};
	};
	fragment@3 {
		target = <&dsi0>;
		__overlay__ {
			status = "okay";
			dsi0_panel: panel@0 {
				backlight = <&backlight>;

				width-mm = <152>;
				height-mm = <91>;
				bpc=<6>;
				bus-format = <0x1009>;//<MEDIA_BUS_FMT_RGB666_1X18>;
				dsi,flags = <0x03>;//101000000011
				dsi,format = <0>;//MIPI_DSI_FMT_RGB666:
				dsi,lanes = <4>;

				t1=<500>;//VCC on to start lvds signal
				t2=<1000>;//t2-t6: LVDS signal(start) to turn Backlihgt on or Backlight sys Enable
				t3=<1000>;//Backlight sys Disable or Backlihgt(off) to stop lvds signal
				t4=<500>;//LVDS signal to turn VCC off
				t5=<1000>;//VCC off to turn VCC on
				t6=<0>;//Backlight sys Enable to turn Backlight on
				t7=<0>;//Backlight off to Backlight sys Disable
				//t1 -> t2-t6 -> t6 -> t3 -> t7 -> t4 -> t5

				disp_timings0: display-timings {
					native-mode = <&dsi0_timing0>;

					dsi0_timing0: timing0  {
						clock-frequency = <33300000>;
						hactive = <800>;
						vactive = <480>;
						hfront-porch = <102>;
						hback-porch = <102>;
						hsync-len = <52>;
						vfront-porch = <18>;
						vsync-len = <9>;
						vback-porch = <18>;
						hsync-active = <0>;
						vsync-active = <0>;
						de-active = <1>;
						pixelclk-active = <0>;
					};
				};
			};
		};
	};
	fragment@4 {
		target = <&edp>;
		__overlay__ {
			status = "disabled";
		};
	};
	fragment@5 {
		target = <&edp_phy>;
		__overlay__ {
			status = "disabled";
		};
	};
	fragment@6 {
		target = <&panel_edp>;
		__overlay__ {
			status = "disabled";
		};
	};
	fragment@7 {
		target = <&dsi0>;
		__overlay__ {
			status = "okay";
		};
	};
	fragment@8 {
		target = <&dsi0_in_vp1>;
		__overlay__ {
			status = "okay";
		};
	};
	fragment@9 {
		target = <&edp_in_vp1>;
		__overlay__ {
			status = "disabled";
		};
	};
};
